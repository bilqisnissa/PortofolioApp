<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/profileFragment">

    <fragment
        android:id="@+id/experienceFragment"
        android:name="com.muflihunnisa.portofolioapp.ExperienceFragment"
        android:label="fragment_experience"
        tools:layout="@layout/fragment_experience" >
        <action
            android:id="@+id/action_experienceFragment_to_detailExperienceFragment"
            app:destination="@id/detailExperienceFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.muflihunnisa.portofolioapp.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/androidFragment"
        android:name="com.muflihunnisa.portofolioapp.AndroidFragment"
        android:label="fragment_android"
        tools:layout="@layout/fragment_android" />
    <fragment
        android:id="@+id/flutterFragment"
        android:name="com.muflihunnisa.portofolioapp.FlutterFragment"
        android:label="fragment_flutter"
        tools:layout="@layout/fragment_flutter" />
    <fragment
        android:id="@+id/uiuxFragment"
        android:name="com.muflihunnisa.portofolioapp.UiuxFragment"
        android:label="fragment_uiux"
        tools:layout="@layout/fragment_uiux" />
    <fragment
        android:id="@+id/detailExperienceFragment"
        android:name="com.muflihunnisa.portofolioapp.DetailExperienceFragment"
        android:label="fragment_detail_experience"
        tools:layout="@layout/fragment_detail_experience" />
</navigation>